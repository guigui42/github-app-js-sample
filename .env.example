# Environment variables for development
# Copy this file to .env and fill in your actual values

# Your GitHub App ID
APP_ID=

# Path to your GitHub App's private key file
PRIVATE_KEY_PATH=

# Your webhook secret
WEBHOOK_SECRET=

# Optional: GitHub Enterprise hostname (omit for GitHub.com)
ENTERPRISE_HOSTNAME=

# Server port (default: 3000)
PORT=3000
